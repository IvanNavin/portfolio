mixin btn(dataName, href, style)
    a(href=href target='_blank')
        svg(xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' viewBox='0 0 500 500')
            defs
                path#textcircle(d='M50,250c0-110.5,89.5-200,200-200s200,89.5,200,200s-89.5,200-200,200S50,360.5,50,250')
            text(dy='70' textLength='1220' style=style)
                textpath(xlink:href='#textcircle' data-btn-text=dataName)